<div class="d-flex" data-bind="if: pagesVisible">
    <ul class="pagination-box" data-bind="foreach: {data: pages, as: 'number'}">
        <li data-bind="css: {'active': $root.current() === number}">
            <a class="text-black" href="javascript: undefined;" data-bind="click: $root.selectedNewPage, text: number"></a>
        </li>
    </ul>
</div>